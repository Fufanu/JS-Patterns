{
    "tags": {
        "allowUnknownTags": true
    },
    "source": {
		"exclude":["./node_modules","./tests"],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": true
        }
    },
	"opts": {
		"template":"./node_modules/minami",
		"encoding":"utf8",
		"recurse":"true",
		"destination":"./doc/"
	}
}
